    "testcafe": "^0.18.6",
    "uppercamelcase": "^1.1.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
